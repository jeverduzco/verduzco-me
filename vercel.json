{
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder",
            "config": {}
        }
    ],
    "trailingSlash": true,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/_nuxt/(.+)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31557600"
                }
            ]
        }
    ]
}